package array;

public class ArrayTest2 {

	public static void main(String[] args) {
		// [행][열]
		int[][] score = {  
				{100, 90, 80, 70},
				{90 , 80, 70, 60},
				{80, 70, 60, 50}
		};
		
		// 행의 개수 : score.length
		for(int row=0; row<score.length; row++) {
			for(int col=0; col<score[row].length; col++) {
				System.out.print(score[row][col] +"\t");
			}
			System.out.println();
		}
		System.out.println("종료");
	}

}
