package control;

public class WhileTest2 {

	public static void main(String[] args) {
		// for문 3개로 구구단 출력 
		
		int testCase = 2;
		while(testCase-->0) {
			int num = 2;
			while(num <= 9) {
				int count = 1;
				while(count<=9) {
					System.out.print(num + "*" + count + "=" + num*count + "\t");
					count++;
				}
				
				System.out.println();
				num += 4;
			}
		}
		
	}

}
