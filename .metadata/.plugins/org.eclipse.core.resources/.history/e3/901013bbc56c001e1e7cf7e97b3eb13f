package control;

public class WhileTest2 {

	public static void main(String[] args) {
		// for문 3개로 구구단 출력 
		
		
		int a = 2;
		while(a<=9) {
			int num = 1;
			
			int temp = a;
			while(temp <= a+4) {
				while(num <= 9) {
					System.out.println(temp +"*"+num+"="+a*num);
					temp++;
					num++;
				}
				temp = a;
			}
			
			a+=4;
			System.out.println();
		}
		
	}

}
