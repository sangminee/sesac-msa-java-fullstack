public class JdbcTest1 {
	public static void main(String[] args) {
		Connection conn = null;
		Statement stmt = null;
		ResultSet rs = null;
		
		try {
			Class.forName("oracle.jdbc.OracleDriver");
			System.out.println("jdbc driver 로드");
		} catch (Exception e) {
			System.out.println("드라이버 로드 실패");
		}
		System.out.println("종료");
	}
}
