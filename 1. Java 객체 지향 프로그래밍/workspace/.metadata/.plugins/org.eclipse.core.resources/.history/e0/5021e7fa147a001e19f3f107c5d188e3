package jdbc;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.Statement;

public class JdbcTest1 {
	public static void main(String[] args) {
		Connection conn = null;
		Statement stmt = null;
		ResultSet rs = null; // 하나의 값으로 다 받아옴 (Begin Of File -- End Of File)
		
		String sql = "SECET employee_id, last_name, salary FROM employees";
		
		try {
			Class.forName("oracle.jdbc.OracleDriver");
			conn = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521/xepdb1","ace", "ace");
			stmt = conn.createStatement();
			rs = stmt.executeQuery(sql);
	
			while(rs.next()) {
				System.out.println(rs.getLong("employee_id"));
			}
			
			System.out.println("connection 성공");
		} catch (Exception e) {
			System.out.println("connection 실패 : " + e.getMessage());
		}
		System.out.println("종료");
	}
}
